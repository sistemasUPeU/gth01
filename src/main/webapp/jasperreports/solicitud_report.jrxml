		<queryString language="SQL">
			<![CDATA[SELECT 'Vacaciones ' || row_number( ) over( order by D.ID_DET_VACACIONES asc) AS ID,
	TO_CHAR(D.FECHA_INICIO,'DD/MM/YYYY') AS FE_INICIO,
	TO_CHAR(D.FECHA_FIN,'DD/MM/YYYY') AS FE_FIN
FROM "RHMV_TRABAJADOR_FILTRADO" A
	JOIN "RHTM_TRABAJADOR" B ON 
	 A."ID_TRABAJADOR" = B."ID_TRABAJADOR" 
	JOIN "RHMV_VACACIONES" C ON 
	 C."ID_TRABAJADOR_FILTRADO" = A."ID_TRABAJADOR_FILTRADO" 
	JOIN "RHMV_DET_VACACIONES" D ON 
	 C."ID_VACACIONES" = D."ID_VACACIONES" 
	 AND C."ESTADO" = 1 
ORDER BY D."ID_DET_VACACIONES" ASC]]>
		</queryString>
